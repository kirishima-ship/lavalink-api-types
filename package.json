{
  "name": "lavalink-api-types",
  "author": {
    "name": "KagChi"
  },
  "main": "dist",
  "module": "dist",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./enums": {
      "import": "./dist/enums/index.mjs",
      "require": "./dist/enums/index.js"
    },
    "./routes": {
      "import": "./dist/routes/index.mjs",
      "require": "./dist/routes/index.js"
    }
  },
  "version": "1.1.1",
  "license": "GPL-3.0",
  "repository": {
    "url": "https://github.com/kirishima-ship/lavalink-api-types"
  },
  "bugs": {
    "url": "https://github.com/kirishima-ship/lavalink-api-types/issues"
  },
  "readme": "https://github.com/kirishima-ship/lavalink-api-types/blob/main/README.md",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "build": "rimraf dist && tsc && gen-esm-wrapper dist/index.js dist/index.mjs",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write {src,tests}/**/*.ts"
  },
  "devDependencies": {
    "@sapphire/eslint-config": "4.3.7",
    "@sapphire/prettier-config": "1.4.3",
    "@sapphire/ts-config": "3.3.4",
    "eslint": "8.18.0",
    "gen-esm-wrapper": "^1.1.3",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "typescript": "4.7.4",
    "unbuild": "0.7.4"
  },
  "prettier": "@sapphire/prettier-config"
}
